<div class="container my-5">
  <h3 class="text-center my-5"> List of All companies that has been saved</h3>
  <table class="table table-striped table-dark">
    <thead class="thead-dark">
      <tr>
        <th scope="col">#</th>
        <th scope="col">Company Name</th>
        <th scope="col">CIN</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.companyName}}</td>
        <td>{{this.CIN}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>